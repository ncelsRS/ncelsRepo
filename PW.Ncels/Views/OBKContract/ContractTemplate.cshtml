@model PW.Ncels.Database.Models.OBK.OBKEntityTemplate

<div class="modal-header">
    @if (@Model.Url == "GetContractTemplatePdf")
    {
        <h3 class="modal-title" id="modal-title">Просмотр договора</h3>
    }
    else
    {
        <h3 class="modal-title" id="modal-title">Просмотр счета на оплату</h3>
    }
</div>
<div class="modal-body" id="modal-body">
    <object data="/OBKContract/@Model.Url?id=@Model.Id" type="application/pdf" style="width:100%;height:550px"></object>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-white" ng-click="cancel()">Закрыть</button>
</div>