@model Guid?

<div class="panel-body">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <table id="tableHistory" dt-options="dtOptions" dt-columns="dtColumns" class="table table-striped table-bordered table-hover dataTable"></table>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function() {
        var value = '@(Model != null ? Model.ToString() : "null")';
        $('#tableHistory').DataTable({
            data: null,
            searching: false,
            columns: [
                { title: "Дата регистрации" },
                { title: "Статус" },
                { title: "Автор" },
                { title: "Описание" }
            ]
        });
        $.ajax({
            processData: false,
            type: "POST",
            url: '/OBKContract/GetHistory?modelId=' + value,
            dataType: 'json',
            cache: false,
            success: function (data) {
                if (data.isSuccess) {
                    $("#tableHistory").DataTable({
                        data: data.result,
                        destroy: true,
                        searching: false,
                        "columns": [
                            { "data": "Created" },
                            { "data": "StatusNameRu" },
                            { "data": "Author" },
                            { "data": "StatusDescriptionRu" }
                        ]
                    });
                }
            },
            error: function (e) {
                alert("Connection Failed. Please Try Again", e);
            }
        });
    });
</script>
