

<div class="pwFileView">
    <div class="modal-header">
        <h4 class="modal-title">Просмотр</h4>
    </div>
    <div class="modal-body">
        <object data="/FileViewHandler.ashx?documentId=@(ViewBag.FileId)&name=@(ViewBag.FileName)" type="application/pdf" style="width:100%;height:400px"></object>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-white" ng-click="cancel()">Закрыть</button>
    </div>
</div>


