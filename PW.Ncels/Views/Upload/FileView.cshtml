

<div class="pwFileView">
    <div class="modal-header" style="padding: 15px 0px 5px 15px;">
        <h4 class="modal-title">Просмотр</h4>
    </div>
    <div class="modal-body">
        <object data="/FileViewHandler.ashx?Id=@(ViewBag.FileId)&name=@(ViewBag.FileName)" type="application/pdf" style="width:100%;height:470px"></object>
    </div>
    <div class="modal-footer" style="margin-top: 0; padding: 10px 20px 10px;">
        <button type="button" class="btn btn-white" ng-click="cancel()">Закрыть</button>
    </div>
</div>


