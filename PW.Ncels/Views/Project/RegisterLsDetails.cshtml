@{
    Layout = "../Shared/_Layout.cshtml";
}
@model PW.Ncels.Database.Models.PriceDetailsModel

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>Создать заявление на проведение экспертизы лекарственного средства</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home")">Домашняя страница</a>
            </li>
            <li>
                <a>Заявления</a>
            </li>
            <li class="active">
                <strong>Создать заявление</strong>
            </li>
        </ol>
    </div>
</div>
<input type="hidden" value="@Model" id="projectId" />
<div class="wrapper wrapper-content animated fadeInRight" ng-controller="registerLsDetail">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        Заявление на проведение экспертизы лекарственного средства
                        <small>Заполните все поля формы</small>
                    </h5>
                    <span class="label label-success pull-right">Новый</span>
                </div>
                <div class="ibox-content ibox-heading">
                    <div class="note-font btn-group">
                        @*<button class="btn btn-default btn-sm" type="button" ng-click="editProject()" security bp-type="project" bp-action="edit" bp-id="{{object.Id}}"><i class="fa fa-save"></i> Сохранить</button>*@
                        @*<button class="btn btn-default btn-sm" type="button" ng-click="editProject()" security bp-type="project" bp-action="edit" bp-id="{{object.Id}}"><i class="fa fa-check-square"></i> Подписать ЭЦП</button>*@
                        @*<button class="btn btn-default btn-sm" type="button" ng-click="send()" security bp-type="project" bp-action="execute" bp-id="{{object.Id}}"><i class="fa fa-send-o"></i> Отправить в ЦОЗ</button>*@
                        <button class="btn btn-default btn-sm" type="button" ng-click="view('@Model.Drug.Id')"><i class="fa fa-eye"></i> Просмотр </button>
                        <button class="btn btn-default btn-sm" type="button" onclick="location.href='@Url.Action("CardDownload", "Upload",new {id=@Model.Drug.Id})'"><i class="fa fa-download"></i> Выгрузить файл на диск</button>
                        @*<button class="btn btn-default btn-sm" type="button" ng-click="send()" security bp-type="project" bp-action="execute" bp-id="{{object.Id}}"><i class="fa fa-trash"></i> Удалить</button>*@
                        <button ng-if="'@Model.IsEdit'"  class="btn btn-default btn-sm" type="button" onclick="location.href='@Url.Action("RegisterLs", "Project",new {id = @Model.Drug.Id})'"><i class="fa fa-edit"></i> Редактировать</button>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="tabs-container">
                                <ul class="nav nav-tabs">
                                    <li class="active"><a data-toggle="tab" href="#tab-1">Общие сведения</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Упаковка</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Полный качественный и количественный состав</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-4">Дополнительная информация</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Производитель(и) лекарственного препарата и участок(и) производства</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-6">Соглашение</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-7">Перечень документов</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-8">Вложения</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-9">Переписка</a></li>
                                </ul>
                                <div class="tab-content">
                                    <div id="tab-1" class="tab-pane active">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Данные по договору
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="row">
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Договор</label>
                                                                        @if (Model.Contract.Number!=null)
                                                                        {
                                                                            <p>@Model.Contract.Number</p>
                                                                        }
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Срок действия</label>
                                                                        @if (Model.Contract.EndDate.HasValue)
                                                                        {
                                                                            <p>@Model.Contract.EndDate.Value.ToString("dd.MM.yyyy")</p>
                                                                        }
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Тип ускоренной регистрации</label>
                                                        <p>@Model.Drug.AccelerationTypeName</p>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>№ письма</label>
                                                        <p>@Model.Drug.AccelerationNumber</p>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Дата письма</label>
                                                        @if (Model.Drug.AccelerationDate.HasValue)
                                                        {
                                                            <p>@Model.Drug.AccelerationDate.Value.ToString("dd.MM.yyyy")</p>
                                                        }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="form-group">
                                                    <label>Обоснование</label>
                                                    <p>@Model.Drug.AccelerationNote</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">
                                                        Торговое название
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="form-group">
                                                            <label>На государственном языке</label>
                                                            <p>@Model.Drug.NameKz</p>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>На русском языке</label>
                                                            <p>@Model.Drug.NameRu</p>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>На английском языке</label>
                                                            <p>@Model.Drug.NameEn</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">
                                                        Международное непатентованное название (МНН)
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="form-group">
                                                            <label>На государственном языке</label>
                                                            <p>@Model.Drug.MnnKz</p>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>На русском языке</label>
                                                            <p>@Model.Drug.MnnRu</p>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>На английском языке</label>
                                                            <p>@Model.Drug.MnnEn</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">
                                                        Торговое название на экспорт (для отечественных производителей)
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <grid-light-object object-id="@Model.Drug.Id" url="/Organization/GetOrganizationType?type=12" dt-id="gridOrganizationType12" dt-selectfun="selectGridOrganizationType12" reload-row="reloadGridOrganizationType12" column-order="1" dt-options="dtOptions1" dt-columns="dtColumns1"></grid-light-object>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-3">
                                                <div class="form-group">
                                                    <label>Наличие патентной защиты</label>
                                                    @if (Model.Drug.IsPatent)
                                                    {
                                                        <p>Да</p>
                                                    }
                                                    else
                                                    {
                                                        <p>Нет</p>
                                                    }

                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="form-group">
                                                    <label>№</label>
                                                    <p>@Model.Drug.PatentNumber</p>
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="form-group">
                                                    <label>Дата</label>
                                                    @if (Model.Drug.PatentDate.HasValue)
                                                    {
                                                        <p>@Model.Drug.PatentDate.Value.ToString("dd.MM.yyyy")</p>
                                                    }
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="form-group">
                                                    <label>Срок действия охранного документа на изобретение или полезную модель (приложить копию документа)</label>
                                                    @if (Model.Drug.PatentExpiryDate.HasValue)
                                                    {
                                                        <p>@Model.Drug.PatentExpiryDate.Value.ToString("dd.MM.yyyy")</p>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">
                                                        Лекарственная форма
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="form-group">
                                                            <label>На государственном языке</label>
                                                            <p>@Model.Drug.LsFormNameKz</p>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>На русском языке</label>
                                                            <p>@Model.Drug.LsFormNameRu</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">
                                                        Анатомо-терапевтическо-химическая классификация (АТХ)
