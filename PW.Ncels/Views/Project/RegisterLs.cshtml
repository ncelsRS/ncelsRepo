@{
    Layout = "../Shared/_Layout.cshtml";
}
@Html.Partial("~/Views/Home/SignView.cshtml")
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>Создать заявление на проведение экспертизы лекарственного средства</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home")">Домашняя страница</a>
            </li>
            <li>
                <a>Заявления</a>
            </li>
            <li class="active">
                <strong>Создать заявление</strong>
            </li>
        </ol>
    </div>
</div>
<input type="hidden" value="@Model" id="projectId" />
<div class="wrapper wrapper-content animated fadeInRight" ng-controller="registerLsGrid">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        Заявление на проведение экспертизы лекарственного средства
                        <small>Заполните все поля формы</small>
                    </h5>
                    <span class="label label-success pull-right">Новый</span>
                </div>
                <div class="ibox-content ibox-heading">
                    <div class="note-font btn-group">
                        <button class="btn btn-default btn-sm" type="button" ng-click="editProject()" security bp-type="project" bp-action="edit" bp-id="{{object.Id}}"><i class="fa fa-save"></i> Сохранить</button>
                        <button class="btn btn-default btn-sm" id ="signBtn" type="button" ng-click="sign('@Model')" security bp-type="project" bp-action="edit" bp-id="{{object.Id}}"><i class="fa fa-check-square"></i> Подписать ЭЦП</button>
                        <button class="btn btn-default btn-sm" type="button" ng-click="sendAgree()" security bp-type="project" bp-action="execute" bp-id="{{object.Id}}"><i class="fa fa-send-o"></i> Отправить в ЦОЗ</button>
                        <button class="btn btn-default btn-sm" type="button" ng-init="false" ng-if="isEnableDownload" ng-click="view('@Model')" @*onclick="location.href='@Url.Action("FileView", "Upload")'"*@><i class="fa fa-eye"></i> Просмотр </button>
                        <button class="btn btn-default btn-sm" type="button" ng-init="false" ng-if="isEnableDownload" onclick="location.href='@Url.Action("CardDownload", "Upload",new {id=@Model})'" security bp-type="project" bp-action="execute" bp-id="{{object.Id}}"><i class="fa fa-download"></i> Выгрузить файл на диск</button>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="tabs-container">
                                <ul class="nav nav-tabs">
                                    <li class="active"><a data-toggle="tab" href="#tab-1">Общие сведения</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Упаковка</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Полный качественный и количественный состав</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-4">Дополнительная информация</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Производитель(и) лекарственного препарата и участок(и) производства</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-6">Соглашение</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-7">Перечень документов</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-8">Вложения</a></li>
                                    @*<li class=""><a data-toggle="tab" href="#tab-9">Переписка</a></li>*@
                                </ul>
                                <div class="tab-content">
                                    <div id="tab-1" class="tab-pane active">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Данные по договору
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="row">
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Договор</label>
                                                                        <ui-select ng-model="object.Drug.ContractId" ui-select-selected required theme="bootstrap" ng-change="setContractDate('ContractId','ContractExpireDate',object.Drug.ContractId)">
                                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                            <ui-select-choices repeat="item.Id as item in ContractId | filter: $select.search">
                                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                            </ui-select-choices>
                                                                        </ui-select>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Срок действия</label>
                                                                        <div class="input-group date">
                                                                            <input ng-model="ContractExpireDate" type="datetime" name="Date" class="form-control" placeholder="ДД.ММ.ГГГГ" ui-select-selected date-time auto-close="true" view="date" min-view="date" format="DD.MM.YYYY" disabled>
                                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Тип ускоренной регистрации</label>
                                                        <ui-select ng-model="object.Drug.AccelerationTypeDicId" ng-change="setSelectedCode('AccelerationType','accelerationSelected',object.Drug.AccelerationTypeDicId)" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in AccelerationType | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4" ng-if="accelerationSelected=='fast'">
                                                    <div class="form-group">
                                                        <label>№ письма</label>
                                                        <input ng-model="object.Drug.AccelerationNumber" placeholder="Введите значение" class="form-control" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-lg-4" ng-if="accelerationSelected=='fast'">
                                                    <div class="form-group">
                                                        <label>Дата письма</label>
                                                        <div class="input-group date">
                                                            <input ng-model="object.Drug.AccelerationDate" type="datetime" name="Date" class="form-control" placeholder="ДД.ММ.ГГГГ" ui-select-selected date-time auto-close="true" view="date" min-view="date" format="DD.MM.YYYY">
                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row" ng-if="accelerationSelected=='fast'">
                                                <div class="col-lg-12">
                                                    <div class="form-group">
                                                        <label>Обоснование</label>
                                                        <input ng-model="object.Drug.AccelerationNote" placeholder="Введите значение" class="form-control" type="text">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Торговое название
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="form-group">
                                                                <label>На государственном языке</label>
                                                                <input ng-model="object.Drug.NameKz" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На русском языке</label>
                                                                <input ng-model="object.Drug.NameRu" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На английском языке</label>
                                                                <input ng-model="object.Drug.NameEn" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Международное непатентованное название (МНН)
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="form-group">
                                                                <label>На государственном языке</label>
                                                                <input ng-model="object.Drug.MnnKz" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На русском языке</label>
                                                                <input ng-model="object.Drug.MnnRu" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На английском языке</label>
                                                                <input ng-model="object.Drug.MnnEn" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Торговое название на экспорт (для отечественных производителей)
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="row">
                                                                <div class="col-lg-3">
                                                                    <div class="form-group">
                                                                        <label>Страна</label>
                                                                        <ui-select ng-model="object.Export.CountryDicId" ui-select-selected required theme="bootstrap">
                                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                            <ui-select-choices repeat="item.Id as item in Country | filter: $select.search">
                                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                            </ui-select-choices>
                                                                        </ui-select>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-3">
                                                                    <div class="form-group">
                                                                        <label>Наименование на государственном языке</label>
                                                                        <input ng-model="object.Export.NameKz" placeholder="Введите значение" class="form-control" type="text">

                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-3">
                                                                    <div class="form-group">
                                                                        <label>Наименование на русском языке</label>
                                                                        <input ng-model="object.Export.NameRu" placeholder="Введите значение" class="form-control" type="text">

                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-3">
                                                                    <div class="form-group">
                                                                        <label>Наименование на английском языке</label>
                                                                        <input ng-model="object.Export.NameEn" placeholder="Введите значение" class="form-control" type="text">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-12">
                                                                    <button class="btn btn-success " type="button" ng-click="addOrganizationType12()"><i class="fa fa-eraser"></i>&nbsp;&nbsp;<span class="bold">Очистить</span></button>
                                                                    <button class="btn btn-success " type="button" ng-click="saveOrganizationType12()"><i class="fa fa-plus"></i>&nbsp;&nbsp;<span class="bold">Сохранить</span></button>
                                                                    <button type="button" class="btn btn-w-m btn-danger" ng-click="deleteOrganizationType12()"><i class="fa fa-minus"></i>&nbsp;&nbsp;<span class="bold">Удалить</span></button>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-12">
                                                                    <grid-light-object object-id="@Model" url="/Organization/GetOrganizationType?type=12" dt-id="gridOrganizationType12" dt-selectfun="selectGridOrganizationType12" reload-row="reloadGridOrganizationType12" column-order="1" dt-options="dtOptions1" dt-columns="dtColumns1"></grid-light-object>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-3">
                                                    <div class="form-group">
                                                        <label>Наличие патентной защиты</label>
                                                        <ui-select ng-model="object.Drug.IsPatent" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in BoolDic | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                                <div class="col-lg-3">
                                                    <div class="form-group">
                                                        <label>№</label>
                                                        <input ng-model="object.Drug.PatentNumber" placeholder="Введите значение" class="form-control" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-lg-3">
                                                    <div class="form-group">
                                                        <label>Дата</label>
                                                        <div class="input-group date">
                                                            <input ng-model="object.Drug.PatentDate" type="datetime" name="Date" class="form-control" placeholder="ДД.ММ.ГГГГ" ui-select-selected date-time auto-close="true" view="date" min-view="date" format="DD.MM.YYYY">
                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-3">
                                                    <div class="form-group">
                                                        <label>Срок действия охранного документа на изобретение или полезную модель (приложить копию документа)</label>
                                                        <div class="input-group date">
                                                            <input ng-model="object.Drug.PatentExpiryDate" type="datetime" name="Date" class="form-control" placeholder="ДД.ММ.ГГГГ" ui-select-selected date-time auto-close="true" view="date" min-view="date" format="DD.MM.YYYY">
                                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Лекарственная форма
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="form-group">
                                                                <label>На государственном языке</label>
                                                                <input ng-model="object.Drug.LsFormNameKz" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На русском языке</label>
                                                                <input ng-model="object.Drug.LsFormNameRu" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Анатомо-терапевтическо-химическая классификация (АТХ)
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="form-group">
                                                                <label>АТХ</label>
                                                                <ui-select ng-model="object.Drug.AtxId" ng-change="setSelectedAtx(object.Drug.AtxId)" ui-select-selected required theme="bootstrap">
                                                                    <ui-select-match>{{$select.selected.Code}} {{$select.selected.Name}}</ui-select-match>
                                                                    <ui-select-choices repeat="item.Id as item in Atx | filter: $select.search">
                                                                        <div ng-bind-html="item.Code + ' ' + item.Name| highlight: $select.search"></div>
                                                                    </ui-select-choices>
                                                                </ui-select>
                                                            </div>
                                                            <div class="form-group">
                                                                <label>Код</label>
                                                                <input ng-model="object.Drug.AtxCode" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                            <div class="form-group">
                                                                <label>На государственном языке</label>
                                                                <input ng-model="object.Drug.AtxNameKz" placeholder="Введите значение" class="form-control" type="text">

                                                            </div>
                                                            <div class="form-group">
                                                                <label>На русском языке</label>
                                                                <input ng-model="object.Drug.AtxNameRu" placeholder="Введите значение" class="form-control" type="text">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Лекарственное средство</label>
                                                        <ui-select ng-model="object.Drug.LsTypeDicId" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in LsType | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>&nbsp;</label>
                                                        <ui-select ng-model="object.Drug.LsType2DicId" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in LsType2 | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Указать название оригинального лекартсвенного препарата</label>
                                                        <input ng-model="object.Drug.OriginalName" placeholder="Введите значение" class="form-control" type="text">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Форма отпуска в стране заявителя</label>
                                                        <ui-select ng-model="object.Drug.SaleTypeDicId" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in SaleType | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Способы введения</label>
                                                        <ui-select ng-model="object.Drug.IntroducingMethodDicId" ui-select-selected required theme="bootstrap">
                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                            <ui-select-choices repeat="item.Id as item in IntroducingMethod | filter: $select.search">
                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                            </ui-select-choices>
                                                        </ui-select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                            Дозировка/концентрация (заполняется при наличии, объем заполняется в упаковке)
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="row">
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Количество</label>
                                                                        <input ng-model="object.Drug.Dosage" placeholder="Введите значение" class="form-control" type="number">
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Единица измерения</label>
                                                                        <ui-select ng-model="object.Drug.DosageMeasureTypeDicId" ui-select-selected required theme="bootstrap">
                                                                            <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                            <ui-select-choices repeat="item.Id as item in MeasureType | filter: $select.search">
                                                                                <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                            </ui-select-choices>
                                                                        </ui-select>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                @*<div class="col-lg-3">
                                                                        <div class="form-group">
                                                                            <label>Описание фасовки (на рус)</label>
                                                                            <input ng-model="object.Drug.DosageNoteRu" placeholder="Введите значение" class="form-control" type="text">
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-lg-3">
                                                                        <div class="form-group">
                                                                            <label>Описание фасовки (на каз)</label>
                                                                            <input ng-model="object.Drug.DosageNoteKz" placeholder="Введите значение" class="form-control" type="text">
                                                                        </div>
                                                                    </div>*@
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Концентрация (на рус)</label>
                                                                        <input ng-model="object.Drug.ConcentrationRu" placeholder="Введите значение" class="form-control" type="text">
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6">
                                                                    <div class="form-group">
                                                                        <label>Концентрация (на каз)</label>
                                                                        <input ng-model="object.Drug.ConcentrationKz" placeholder="Введите значение" class="form-control" type="text">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="tab-2" class="tab-pane">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="row">
                                                        <div class="col-lg-6">
                                                            <div class="form-group">
                                                                <label>Вид</label>
                                                                <ui-select ng-model="object.Package.PackageTypeDicId" ui-select-selected required theme="bootstrap">
                                                                    <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                    <ui-select-choices repeat="item.Id as item in PackageType | filter: $select.search">
                                                                        <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                    </ui-select-choices>
                                                                </ui-select>
                                                            </div>
