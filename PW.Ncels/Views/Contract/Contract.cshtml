                                                            <input ng-model="object.Manufacture.Iin" placeholder="Введите значение" class="form-control" type="text" ui-mask="999999999999" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
