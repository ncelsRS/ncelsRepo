@using PW.Ncels.Database.DataModel
@model Guid
@{
    Layout = "../Shared/_Layout.cshtml";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>Новый договор</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home")">Домашняя страница</a>
            </li>
            <li>
                <a>Договоры</a>
            </li>
            <li class="active">
                <strong>Создать договор</strong>
            </li>
        </ol>
    </div>
</div>
<input type="hidden" value="@Model" id="projectId" />
<div class="wrapper wrapper-content animated fadeInRight" ng-controller="contractForm">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        Новый договор
                        <small>Заполните все поля формы</small>
                    </h5>
                    <div ibox-tools></div>
                </div>
                <div class="ibox-content ibox-heading">
                    <div class="note-font btn-group">
                        <button class="btn btn-info btn-sm" type="button" onclick="location.href='@Url.Action(ViewBag.ListAction, "Contract")'"><i class="fa fa-arrow-left"></i> Вернуться в список</button>
                        <button class="btn btn-default btn-sm" type="button" ng-click="editProject()" check-validate ng-if="!object.Contract.ContractStatus || object.Contract.ContractStatus.Code=='@Contract.StatusNew' || object.Contract.ContractStatus.Code=='@Contract.StatusOnCorrection'">
                            <i class="fa fa-save"></i>
                            Сохранить
                        </button>                        
                        <button class="btn btn-default btn-sm" type="button" ng-click="sendInProcessing()" ng-if="object.Contract.ContractStatus.Code=='@Contract.StatusNew' || object.Contract.ContractStatus.Code=='@Contract.StatusOnCorrection'"><i class="fa fa-send-o"></i> Отправить в ЦОЗ</button>
                        <button class="btn btn-default btn-sm" type="button" ng-if="object.Contract.ContractStatus" ng-click="viewContract('@Model')"><i class="fa fa-eye"></i> Просмотр </button>                        
                    </div>
                </div>
                <form name="contractCreateForm">
                    <div class="ibox-content">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-horizontal row">
                                    <div class="col-lg-5">
                                        <div class="form-group">
                                            <label class="control-label col-lg-4">Договор составляется с</label>
                                            <div class="col-lg-8">
                                                <ui-select ng-model="object.Contract.HolderTypeId" ui-select-selected required theme="bootstrap" ng-change="contractHolderChanged($select)">
                                                    <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                    <ui-select-choices repeat="item.Id as item in ContractHolderType | filter: $select.search">
                                                        <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                    </ui-select-choices>
                                                </ui-select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-5">
                                        <div class="form-group">
                                            <label class="control-label col-lg-4">Подписант со стороны НЦЭЛС</label>
                                            <div class="col-lg-8">
                                                <ui-select ng-model="object.Contract.SignerId" ui-select-selected required theme="bootstrap">
                                                    <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                    <ui-select-choices repeat="item.Id as item in Signers | filter: $select.search">
                                                        <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                    </ui-select-choices>
                                                </ui-select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label class="control-label col-lg-6">Подписывать договор ЭЦП</label>
                                            <div class="col-lg-6">
                                                <ui-select ng-model="object.Contract.IsHasDigCert" required ui-select-selected theme="bootstrap">
                                                    <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                    <ui-select-choices repeat="item.Id as item in BoolDic | filter: $select.search">
                                                        <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                    </ui-select-choices>
                                                </ui-select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tabs-container">
                                    <ul class="nav nav-tabs">
                                        <li class="active"><a data-toggle="tab" href="#tab-1">Производитель</a></li>
                                        <li class=""><a data-toggle="tab" href="#tab-2">Заявитель</a></li>
                                        @*<li class=""><a data-toggle="tab" href="#tab-3">Доверенное лицо</a></li>*@
                                        <li class=""><a data-toggle="tab" href="#tab-4">Держатель регистрационного удостоверения</a></li>
                                        <li class=""><a data-toggle="tab" href="#tab-5">Плательщик</a></li>
                                        <li class=""><a data-toggle="tab" href="#tab-6">Плательщик за перевод</a></li>
                                        <li class=""><a data-toggle="tab" href="#tab-7">Вложения</a></li>
                                        <li ng-if="object.Contract.ContractStatus && object.Contract.ContractStatus.Code!=='@Contract.StatusNew'" class=""><a data-toggle="tab" href="#tab-8"><h3 style="color: red">Замечания</h3></a></li>
                                    </ul>
                                    <div class="tab-content">
                                        <div id="tab-1" class="tab-pane active">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-lg-6 b-r">
                                                        <div class="row">
                                                            <button class="btn btn-default col-lg-12" type="button" ng-click="clearOrgTabData(manufacturesSource, 'Manufacture', 'manufacture')"><i class="glyphicon glyphicon-erase"></i> Очистить данные производителя</button>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Выбрать производителя из справочника</label>
                                                            <div style="color: maroon" role="alert">«В случае отсутствия наименования производителя в справочнике, необходимо заполнить поле «Наименование на русском языке», «Наименование на английском языке»</div>
                                                            <ui-select ng-model="manufacturesSource.selected" theme="bootstrap" ng-change="fetchOrganization('manufacturesSource', object.Manufacture, copyManufactureFields,'manufacture')">
                                                                <ui-select-match placeholder="Введите наименование производителя для поиска в справочнике">{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item in manufacturesSource.items | filter: $select.search" refresh="fetchOrganizations($select, 'manufacturesSource')" refresh-delay="1000">
                                                                    <div ng-bind-html="item.Name"></div>
                                                                    <div ng-if="$index == $select.items.length-1">
                                                                        <button class="btn btn-xs btn-success" ng-click="fetchOrganizations($select, 'manufacturesSource', $event)"
                                                                                style="width: 100%; margin-top: 5px;"
                                                                                ng-disabled="manufacturesSource.loading || manufacturesSource.page>manufacturesSource.lastPage">
                                                                            Загрузить еще...
                                                                        </button>
                                                                    </div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Организационная форма</label>
                                                            <ui-select name="object.Manufacture.OpfTypeDicId" ng-model="object.Manufacture.OpfTypeDicId" theme="bootstrap" ng-change="setSelectedCode('OpfType','manufactureOpfType',object.Manufacture.OpfTypeDicId);syncOrgs(object.Manufacture,'manufacture');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in OpfType | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.NameRu">Наименование на русском языке *</label>
                                                            <input name="object.Manufacture.NameRu" ng-model="object.Manufacture.NameRu" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.NameEn">Наименование на английском языке *</label>
                                                            <div ng-show="contractCreateForm.manufactureEnName.$invalid" style="color: maroon" role="alert">В наименование должны быть только символы английского алфавита</div>
                                                            <input name="manufactureEnName" ng-model="object.Manufacture.NameEn" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-pattern="enName" ng-model-options="{allowInvalid:true}" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.CountryDicId">Страна *</label>
                                                            <ui-select name="object.Manufacture.CountryDicId" ng-model="object.Manufacture.CountryDicId" ui-select-selected required theme="bootstrap" ng-change="setSelectedCode('Country','manufactureCountry',object.Manufacture.CountryDicId);syncOrgs(object.Manufacture,'manufacture');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in Country | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.AddressLegal">Юридический адрес *</label>
                                                            <input name="object.Manufacture.AddressLegal" ng-model="object.Manufacture.AddressLegal" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.AddressFact">Фактический адрес *</label>
                                                            <input name="object.Manufacture.AddressFact" ng-model="object.Manufacture.AddressFact" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.Phone">Телефон *</label>
                                                            <input name="object.Manufacture.Phone" ng-model="object.Manufacture.Phone" class="form-control required" type="text" ng-model-options="{allowInvalid:true}" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.Email">e-mail *</label>
                                                            <input name="object.Manufacture.Email" ng-model="object.Manufacture.Email" placeholder="example@mail.ru" class="form-control" type="text" ui-select-selected required ng-pattern="emailPattern" ng-model-options="{allowInvalid:true}" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 b-l">
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.BossLastName">Фамилия первого руководителя *</label>
                                                            <input name="object.Manufacture.BossLastName" ng-model="object.Manufacture.BossLastName" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.BossFirstName">Имя первого руководителя *</label>
                                                            <input name="object.Manufacture.BossFirstName" ng-model="object.Manufacture.BossFirstName" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Отчество первого руководителя</label>
                                                            <input name="object.Manufacture.BossMiddleName" ng-model="object.Manufacture.BossMiddleName" placeholder="Введите значение" class="form-control" type="text" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Должность первого руководителя</label>
                                                            <input name="object.Manufacture.BossPosition" ng-model="object.Manufacture.BossPosition" placeholder="Введите значение" class="form-control" type="text" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Manufacture.BankName">Название банка *</label>
                                                            <input name="object.Manufacture.BankName" ng-model="object.Manufacture.BankName" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureCountry=='KZ'">
                                                            <label>ИИК</label>
                                                            <input name="object.Manufacture.BankIik" ng-model="object.Manufacture.BankIik" placeholder="Введите значение" class="form-control" type="text" ui-mask="********************" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureCountry!='KZ'">
                                                            <label>Расчетный счет</label>
                                                            <input name="object.Manufacture.PaymentBill" ng-model="object.Manufacture.PaymentBill" placeholder="Введите значение" class="form-control" type="text" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Валюта</label>
                                                            <ui-select name="object.Manufacture.BankCurencyDicId" ng-model="object.Manufacture.BankCurencyDicId" theme="bootstrap" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in Currency | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureCountry!='KZ'">
                                                            <label>SWIFT</label>
                                                            <input name="object.Manufacture.BankSwift" ng-model="object.Manufacture.BankSwift" placeholder="Введите значение" class="form-control" type="text" ui-mask="********" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureCountry=='KZ'">
                                                            <label>БИК</label>
                                                            <input name="object.Manufacture.BankBik" ng-model="object.Manufacture.BankBik" placeholder="Введите значение" class="form-control" type="text" ui-mask="AAAAAAAA" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureCountry=='KZ'">
                                                            <label>БИН</label>
                                                            <input name="object.Manufacture.Bin" ng-model="object.Manufacture.Bin" placeholder="Введите значение" class="form-control" type="text" ui-mask="999999999999" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                        <div class="form-group" ng-if="manufactureOpfType=='fl'">
                                                            <label>ИИН</label>
                                                            <input name="object.Manufacture.Iin" ng-model="object.Manufacture.Iin" placeholder="Введите значение" class="form-control" type="text" ui-mask="999999999999" ui-mask-placeholder ui-mask-placeholder-char="_" ng-change="syncOrgs(object.Manufacture,'manufacture');">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="tab-2" class="tab-pane">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-lg-6 b-r">
                                                        <div class="checkbox">
                                                            <label>
                                                                <input type="checkbox" ng-model="applicantIsManufacture" ng-change="applicantIsManufactureChanged(applicantIsManufacture)"> Является производителем
                                                            </label>
                                                        </div>
                                                        <div class="row">
                                                            <button class="btn btn-default col-lg-12" type="button" ng-click="clearOrgTabData(applicantSource, 'Applicant', 'applicant')"><i class="glyphicon glyphicon-erase"></i> Очистить данные заявителя</button>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Выбрать заявителя из справочника</label>
                                                            <div style="color:maroon" role="alert">«В случае отсутствия наименования заявителя в справочнике, необходимо заполнить поле «Наименование на русском языке», «Наименование на английском языке»</div>
                                                            <ui-select ng-model="applicantSource.selected" theme="bootstrap" ng-change="fetchOrganization('applicantSource', object.Applicant, copyApplicantFields,'applicant')">
                                                                <ui-select-match placeholder="Введите наименование заявителя для поиска в справочнике">{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item in applicantSource.items | filter: $select.search" refresh="fetchOrganizations($select, 'applicantSource')" refresh-delay="1000">
                                                                    <div ng-bind-html="item.Name"></div>
                                                                    <div ng-if="$index == $select.items.length-1">
                                                                        <button class="btn btn-xs btn-success" ng-click="fetchOrganizations($select, 'applicantSource', $event)"
                                                                                style="width: 100%; margin-top: 5px;"
                                                                                ng-disabled="applicantSource.loading || applicantSource.page>applicantSource.lastPage">
                                                                            Загрузить еще...
                                                                        </button>
                                                                    </div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Организационная форма</label>
                                                            <ui-select name="object.Applicant.OpfTypeDicId" ng-model="object.Applicant.OpfTypeDicId" theme="bootstrap" ng-change="setSelectedCode('OpfType','applicantOpfType',object.Applicant.OpfTypeDicId);syncOrgs(object.Applicant,'applicant');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in OpfType | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.NameRu">Наименование на русском языке *</label>
                                                            <input name="object.Applicant.NameRu" ng-model="object.Applicant.NameRu" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Наименование на английском языке</label>
                                                            <div ng-show="contractCreateForm.applicantEnName.$invalid" style="color:maroon" role="alert">В наименование должны быть только символы английского алфавита</div>
                                                            <input name="applicantEnName" ng-model="object.Applicant.NameEn" placeholder="Введите значение" class="form-control" type="text" ng-pattern="enName" ng-model-options="{allowInvalid:true}" ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.CountryDicId">Страна *</label>
                                                            <ui-select name="object.Applicant.CountryDicId" ng-model="object.Applicant.CountryDicId" ui-select-selected required theme="bootstrap" ng-change="setSelectedCode('Country','applicantCountry',object.Applicant.CountryDicId);syncOrgs(object.Applicant,'applicant');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in Country | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.AddressLegal">Юридический адрес *</label>
                                                            <input name="object.Applicant.AddressLegal" ng-model="object.Applicant.AddressLegal" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.AddressFact">Фактический адрес *</label>
                                                            <input name="object.Applicant.AddressFact" ng-model="object.Applicant.AddressFact" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.Phone">Телефон *</label>
                                                            <input name="object.Applicant.Phone" ng-model="object.Applicant.Phone" class="form-control required" type="text" ng-model-options="{allowInvalid:true}" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.Email">e-mail *</label>
                                                            <input name="object.Applicant.Email" ng-model="object.Applicant.Email" placeholder="example@mail.ru" class="form-control" type="text" ui-select-selected required ng-pattern="emailPattern" ng-model-options="{allowInvalid:true}" ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 b-l">
                                                        <div class="form-group">
                                                            <label for="object.Applicant.BossLastName">Фамилия подписывающего лица *</label>
                                                            <input name="object.Applicant.BossLastName" ng-model="object.Applicant.BossLastName" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="object.Applicant.BossFirstName">Имя подписывающего лица *</label>
                                                            <input name="object.Applicant.BossFirstName" ng-model="object.Applicant.BossFirstName" placeholder="Введите значение" class="form-control" type="text" ui-select-selected required ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Отчество подписывающего лица</label>
                                                            <input name="object.Applicant.BossMiddleName" ng-model="object.Applicant.BossMiddleName" placeholder="Введите значение" class="form-control" type="text" ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Должность подписывающего лица</label>
                                                            <input name="object.Applicant.BossPosition" ng-model="object.Applicant.BossPosition" placeholder="Введите значение" class="form-control" type="text" ng-change="syncOrgs(object.Applicant,'applicant');">
                                                        </div>
