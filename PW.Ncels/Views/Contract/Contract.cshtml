                                                                <ui-select-choices repeat="item in manufacturesSource.items | filter: $select.search" refresh="fetchOrganizations($select, 'manufacturesSource')" refresh-delay="1000">
                                                                    <div ng-bind-html="item.Name"></div>
                                                                    <div ng-if="$index == $select.items.length-1">
                                                                        <button class="btn btn-xs btn-success" ng-click="fetchOrganizations($select, 'manufacturesSource', $event)"
                                                                                style="width: 100%; margin-top: 5px;"
                                                                                ng-disabled="manufacturesSource.loading || manufacturesSource.page>manufacturesSource.lastPage">
                                                                            Загрузить еще...
                                                                        </button>
                                                                    </div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label>Организационная форма</label>
                                                            <ui-select ng-model="object.Manufacture.OpfTypeDicId" theme="bootstrap" ng-change="setSelectedCode('OpfType','manufactureOpfType',object.Manufacture.OpfTypeDicId);syncOrgs(object.Manufacture,'manufacture');">
                                                                <ui-select-match>{{$select.selected.Name}}</ui-select-match>
                                                                <ui-select-choices repeat="item.Id as item in OpfType | filter: $select.search">
                                                                    <div ng-bind-html="item.Name| highlight: $select.search"></div>
                                                                </ui-select-choices>
                                                            </ui-select>
                                                        </div>
