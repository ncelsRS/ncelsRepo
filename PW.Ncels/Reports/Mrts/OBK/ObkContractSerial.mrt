      <OBK_RS_Products Ref="9" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>OBK_RS_Products</Alias>
        <Columns isList="true" count="23">
          <value>Id,System.Int32</value>
          <value>RegTypeId,System.Int32</value>
          <value>DegreeRiskId,System.Int32</value>
          <value>DrugFormBoxCount,System.String</value>
          <value>DrugFormFullName,System.String</value>
          <value>DrugFormFullNameKz,System.String</value>
          <value>CurrencyId,System.Int64</value>
          <value>NameRu,System.String</value>
          <value>NameKz,System.String</value>
          <value>ProducerNameRu,System.String</value>
          <value>ProducerNameKz,System.String</value>
          <value>CountryNameRu,System.String</value>
          <value>CountryNameKZ,System.String</value>
          <value>TnvedCode,System.String</value>
          <value>KpvedCode,System.String</value>
          <value>Price,System.String</value>
          <value>ContractId,System.Int64</value>
          <value>ProdSeries,System.String</value>
          <value>ProdSeriesEndDate,System.String</value>
          <value>ProdSeriesParty,System.String</value>
          <value>ProdShortName,System.String</value>
          <value>ProdProducerNameRu,System.String</value>
          <value>ProdCountryNameRu,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>OBK_RS_Products</Name>
        <NameInSource>ncelsProd</NameInSource>
        <Parameters isList="true" count="1">
          <value>ContractId,ContractId,14,0</value>
        </Parameters>
        <SqlCommand>select
	(case RegTypeId
	when 1
	then NameRu
	when 2
	then NameRu + ' ' + 
(select s.pName from 
(select ProductId, (
	select a.Name + N', '
				from dbo.OBK_MtPart AS a 
				where mp.ProductId = a.ProductId 
				for xml path('')) as pName
		from OBK_MtPart as mp where mp.ProductId = p.Id  
		group by ProductId) as s)
	end) as NameRu,
 p.Id, ps.OBK_RS_ProductsId, ps.Id, ps.Series as ProdSeries, ps.SeriesEndDate as ProdSeriesEndDate, SeriesParty as ProdSeriesParty, p.CountryNameRu as ProdCountryNameRu, p.ProducerNameRu as ProdProducerNameRu, s.short_name as ProdShortName
from OBK_RS_Products p inner join OBK_Procunts_Series ps on p.Id = ps.OBK_RS_ProductsId
inner join OBK_Contract c on c.Id = p.ContractId
inner join sr_measures s on ps.SeriesMeasureId = s.id
 where c.Id=@ContractId</SqlCommand>
      </OBK_RS_Products>
