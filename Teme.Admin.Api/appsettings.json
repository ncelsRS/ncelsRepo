{
  "ConnectionStrings": {
    //"DefaultConnection": "Server=10.20.44.57;Database=teme;User Id=sa;Password=Aa123456+-;"
    "DefaultConnection": "data source=localhost\\\\SQLEXPRESS;initial catalog=teme;integrated security=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "LiterateConsole" } //,
      //{
      //  "Name": "RollingFile",
      //  "Args": { "pathFormat": "logs/log-{Date}.txt" }
      //}
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
