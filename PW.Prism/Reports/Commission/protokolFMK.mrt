      <CommissionOtherUnitsConcatinate2 Ref="7" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>CommissionOtherUnitsConcatinate2</Alias>
        <Columns isList="true" count="1">
          <value>Users,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>CommissionOtherUnitsConcatinate2</Name>
        <NameInSource>prism</NameInSource>
        <Parameters isList="true" count="1">
          <value>commissionId,CommissionId,8,0</value>
        </Parameters>
        <SqlCommand>SELECT STUFF((
	SELECT ', ' + e.LastName
	+ CASE WHEN LEN(e.FirstName) &gt; 1 
       THEN ' ' + SUBSTRING(e.FirstName, 1, 1) + '.'
       ELSE  '' END  
	+ CASE WHEN LEN(e.MiddleName) &gt; 1 
       THEN ' ' + SUBSTRING(e.MiddleName, 1, 1) + '.'
       ELSE  '' END  
	+ CASE WHEN CU.UnitTypeId = 2 
       THEN ' (' + UT.Name + ')'
       ELSE  '' END  

from dbo.Commissions AS C
  left join dbo.CommissionUnits AS CU ON CU.Commissionid = C.Id 
  	AND CU.UnitTypeId &lt;&gt; 1 AND CU.UnitTypeId &lt;&gt; 3
  left join dbo.Employees AS E ON E.Id = CU.EmployeeId
  left join dbo.CommissionUnitTypes AS UT ON UT.Id = CU.UnitTypeId
	where C.Id = @commissionId
	ORDER BY CU.UnitTypeId
FOR XML PATH ('')
), 1, 2, '') AS Users</SqlCommand>
      </CommissionOtherUnitsConcatinate2>
      <CommissionDrugDeclaration Ref="8" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>CommissionDrugDeclaration</Alias>
        <Columns isList="true" count="5">
          <value>TypeId,System.Int32</value>
          <value>StatusId,System.Int32</value>
          <value>Count,System.Int32</value>
          <value>TypeName,System.String</value>
          <value>TypeList,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>CommissionDrugDeclaration</Name>
        <NameInSource>prism</NameInSource>
        <Parameters isList="true" count="1">
          <value>commissionId,CommissionId,8,0</value>
        </Parameters>
        <SqlCommand>SELECT C.TypeId
	,C.StatusId
	,C.Count
	,T.Name AS TypeName
	,TypeList
	FROM (
SELECT CD.ConclusionTypeId AS TypeId
	,D.StatusId AS StatusId
	,COUNT(1) AS Count
	FROM dbo.CommissionDrugDosage AS CD
	INNER JOIN dbo.EXP_DrugDosage AS DD ON DD.Id = CD.DrugDosageId
	INNER JOIN dbo.EXP_DrugDeclaration AS D ON D.Id = DD.DrugDeclarationId
        WHERE CD.CommissionId = @commissionId
            AND CD.ConclusionTypeId IS NOT NULL            
        GROUP BY CD.ConclusionTypeId, D.StatusId
	) AS C
	INNER JOIN dbo.CommissionConclusionTypes AS T
		ON T.Id = C.TypeId
	INNER JOIN 
		(
SELECT
	ConclusionTypeId,
	STUFF(
	(SELECT DISTINCT ',' + 
         	CASE TypeId
                WHEN 1 THEN 'Р'
                WHEN 2 THEN 'ПР'
                WHEN 3 THEN 'ВИ'
                ELSE '?' 
            END +' - '+ CONVERT(nvarchar(max), count(1))
          FROM dbo.CommissionDrugDosageCountView
          WHERE ConclusionTypeId = a.ConclusionTypeId
          GROUP BY ConclusionTypeId, TypeId
          FOR XML PATH (''))
          , 1, 1, '')  AS TypeList
FROM CommissionDrugDosageCountView AS a
WHERE a.CommissionId = @commissionId
GROUP BY ConclusionTypeId
		) AS X ON X.ConclusionTypeId = C.TypeId</SqlCommand>
      </CommissionDrugDeclaration>
