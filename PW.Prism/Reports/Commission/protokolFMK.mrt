        <NameInSource>prism</NameInSource>
        <Parameters isList="true" count="1">
          <value>commissionid,CommissionId,8,0</value>
        </Parameters>
        <SqlCommand>select C.*
	,E.LastName AS PredsedatelLastName
	,E.FirstName AS PredsedatelFirstName
	,E.MiddleName AS PredsedatelMiddleName
	,E2.LastName AS SekretarLastName
	,E2.FirstName AS SekretarFirstName
	,E2.MiddleName AS SekretarMiddleName
from dbo.Commissions AS C
	left join dbo.CommissionUnits AS CU ON CU.Commissionid = C.Id 
		AND CU.UnitTypeId = 1
	left join dbo.Employees AS E ON E.Id = CU.EmployeeId
	left join dbo.CommissionUnits AS CU2 ON CU2.Commissionid = C.Id 
		AND CU2.UnitTypeId = 3
	left join dbo.Employees AS E2 ON E2.Id = CU2.EmployeeId
where C.Id = @commissionid
</SqlCommand>
