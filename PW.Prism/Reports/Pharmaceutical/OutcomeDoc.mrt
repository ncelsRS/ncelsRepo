        <Parameters isList="true" count="1">
          <value>PharmaceuticalFinalDocId,PharmaceuticalFinalDocId,14,0</value>
        </Parameters>
        <SqlCommand>select dw.*
	,b.name as boxName
	,wt.NameRu as wrappingTypeName
from dbo.EXP_ExpertisePharmaceuticalFinalDoc as epfd
left join dbo.EXP_ExpertiseStageDosage as esd on esd.Id = epfd.DosageStageId 
left join dbo.EXP_DrugDosage as ddos on ddos.Id = esd.DosageId
left join dbo.EXP_DrugWrapping as dw on dw.DrugDosageId = ddos.Id
left join dbo.sr_boxes as b on b.Id = dw.WrappingKindId
left join dbo.EXP_DIC_WrappingType as wt on wt.Id = dw.WrappingTypeId
where epfd.Id = @PharmaceuticalFinalDocId</SqlCommand>
