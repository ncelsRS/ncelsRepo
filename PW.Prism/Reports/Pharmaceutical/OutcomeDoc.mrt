        <Parameters isList="true" count="1">
          <value>PharmaceuticalFinalDocId,PharmaceuticalFinalDocId,14,0</value>
        </Parameters>
        <SqlCommand>select epfd.* 
	,esd.ResultId as ResultId
	,ddos.RegNumber as RegNumber
	,ddos.ConcentrationRu as ConcentrationRu
	,ddos.Dosage as Dosage
	,dst.NameRu as SaleType
	,dd.NameRu as NameRu
	,atc.code as AtxCode
	,atc.name as AtxName
	,df.full_name as DrugFormRu
	,sin.name_rus as MnnName
	,u.Name as OrganizationName
	,u.LegalAddress as OrganizationLegalAddress
	,d.Name as CountryName
	,dt.DrugName as DrugName
	,ddt.NameRu as DrugTypeName
	,ddtk.NameRu as DrugTypeKindName
from dbo.EXP_ExpertisePharmaceuticalFinalDoc as epfd
left join dbo.EXP_ExpertiseStageDosage as esd on esd.Id = epfd.DosageStageId
left join dbo.EXP_DrugDosage as ddos on ddos.Id = esd.DosageId
left join dbo.EXP_DIC_SaleType as dst on dst.Id = ddos.SaleTypeId
left join dbo.EXP_DrugDeclaration as dd on dd.Id = ddos.DrugDeclarationId
left join dbo.sr_drug_forms as df on df.Id = dd.DrugFormId
left join dbo.sr_atc_codes as atc on atc.Id = dd.AtxId 
left join dbo.sr_international_names as sin on sin.Id = dd.MnnId
left join dbo.Employees as emp on emp.Id = dd.OwnerId
left join dbo.Units as u on u.Id = emp.OrganizationId
left join dbo.Dictionaries as d on d.Id = u.CountryId
left join dbo.EXP_DrugType as dt on dt.DrugDeclarationId = dd.Id
left join dbo.EXP_DIC_DrugType as ddt on ddt.Id = dt.DrugTypeId
left join dbo.EXP_DIC_DrugTypeKind as ddtk on ddtk.Id = dt.DrugTypeKind
where epfd.Id = @PharmaceuticalFinalDocId</SqlCommand>
