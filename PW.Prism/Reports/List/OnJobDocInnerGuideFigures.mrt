          <value>OnJobMany,System.Int32</value>
          <value>DepartmentParent,System.String</value>
          <value>ExDateExclude,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Documents</Name>
        <NameInSource>prism</NameInSource>
        <Parameters isList="true" count="3">
          <value>_x0040_dateStart,DateStart,31,0</value>
          <value>_x0040_dateEnd,DateEnd,31,0</value>
          <value>_x0040_organizationId,OrganizationId,12,1000</value>
        </Parameters>
        <SqlCommand>SELECT 
(case when depParent1.Name is not null  then depParent1.Name else case when depParent.Name is not null then depParent.Name else dep.Name end end ) as DepartmentParent,
	dep.Name as Department,
	count(*) as AllCount,
	sum (case when StateType = 9 then 1 else 0 end) as Exclude,
sum (case when Cast(FactExecutionDate as date) &gt;  AutoExecutionDate and StateType = 9 then 1 else 0 end) as FulfilledDateExclude,
sum (case when Cast(FactExecutionDate as date) &lt;= AutoExecutionDate and StateType = 9 then 1 else 0 end) as ExDateExclude,
sum (case when StateType &gt; 1 and StateType &lt; 9  then 1 else 0 end) as OnJob,
sum (case when StateType &gt; 1 and StateType &lt; 9 and AutoExecutionDate &lt; cast( GETDATE() as date)  then 1 else 0 end) as ExecutionDateOnJob,
sum (case when StateType &gt; 1 and StateType &lt; 9 and AutoExecutionDate &gt; cast(GETDATE ()as date)  then 1 else 0 end) as NowOnJob,
sum (case when StateType &gt; 1 and StateType &lt; 9 and DATEDIFF(day, ExecutionDate, GETDATE()) &gt;= 0 and DATEDIFF(day, ExecutionDate, GETDATE()) &lt;= 3   then 1 else 0 end) as OnJob3,
sum (case when StateType &gt; 1 and StateType &lt; 9 and DATEDIFF(day, ExecutionDate, GETDATE()) &lt;= 5 and DATEDIFF(day, ExecutionDate, GETDATE()) &gt; 3  then 1 else 0 end) as OnJob5,
sum (case when StateType &lt;&gt; 9 and DATEDIFF(day,GETDATE() , ExecutionDate)  &gt; 5  then 1 else 0 end) as OnJobMany
  FROM [Documents] as d
left join (select * from  LinkUnits as unis where unis.PropertyName='ResponsibleId') as lin on lin.OwnerId = d.Id 
  left join Employees as e on e.Id = lin.EmployeeId 
  left join Units as dep on dep.Id = lin.Department1Id
  left join Units as depParent on depParent.Id = lin.Department2Id and lin.Department2Id is not null
  left join Units as depParent1 on depParent1.Id = lin.Department3Id and lin.Department3Id is not null
  where StateType &gt; 1 and DocumentType = 5 and MonitoringType in (2,3,4) and AutoDocumentDate between @dateStart and @dateEnd and d.OrganizationId = @organizationId
	and 	 d.ApplicantType = 1	
  group by (case when depParent1.Name is not null  then depParent1.Name else case when depParent.Name is not null then depParent.Name else dep.Name end end ), dep.Name</SqlCommand>
      </Documents>
