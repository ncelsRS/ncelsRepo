      <Employees Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Employees</Alias>
        <Columns isList="true" count="8">
          <value>fil,System.String</value>
          <value>dep,System.String</value>
          <value>otd,System.String</value>
          <value>sec,System.String</value>
          <value>FullName,System.String</value>
          <value>EmployeeId,System.Guid</value>
          <value>DateFile,System.DateTime</value>
          <value>Position,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Employees</Name>
        <NameInSource>prism2015</NameInSource>
        <Parameters isList="true" count="2">
          <value>_x0040_organizationId,OrganizationId,12,1000</value>
          <value>_x0040_departamentId,DepartamentId,12,100</value>
        </Parameters>
        <SqlCommand>SELECT 

	(case  when F.Name is null then (case when D.Name is null then (case when O.Name is null then S.Name else O.Name end) else D.Name end) else F.Name end) AS fil,
(case when F.Name is not null then D.Name else (case when D.Name is not null then O.Name else (case when O.Name is not null then S.Name end ) end) end) as dep,
(case when F.Name is not null and D.Name is not null  then O.Name else (case when F.Name is null and D.Name is not null then S.Name end) end) as otd,
(case when F.Name is not null and D.Name is not null and O.Name is not null then S.Name end) as sec,
		E.FullName, convert(UNIQUEIDENTIFIER, E.Id) EmployeeId, E.DateFile , P.Name as Position
	
from Employees E

left join Units P on P.Id = E.PositionId
left join Units S on S.Id = P.ParentId
left join Units O on O.Id = S.ParentId
left join Units D on D.Id = O.ParentId
left join Units F on F.Id = D.ParentId

where  E.OrganizationId = @organizationId  and P.PositionState != 5 and (s.Id =cast(@departamentId as uniqueidentifier ) or O.Id =cast(@departamentId as uniqueidentifier ) or D.Id = cast(@departamentId as uniqueidentifier ) or F.Id = cast(@departamentId as uniqueidentifier ) )

order by (case when F.Rank is not null then D.Rank else (case when D.Rank is not null then O.Rank else (case when O.Rank is not null then S.Rank end ) end) end),
		(case when F.Rank is not null and D.Rank is not null  then O.Rank else (case when F.Rank is null and D.Rank is not null then S.Rank end) end),
		(case when F.Rank is not null and D.Rank is not null and O.Rank is not null then S.Rank end),P.Rank, E.FullName
		
</SqlCommand>
      </Employees>
      <Holidays Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
