left join dbo.EXP_DrugDosage as ddos on ddos.Id = esd.DosageId
left join dbo.EXP_DIC_SaleType as dst on dst.Id = ddos.SaleTypeId
left join dbo.EXP_DrugDeclaration as dd on dd.Id = ddos.DrugDeclarationId
left join dbo.sr_international_names as sin on sin.Id = dd.MnnId
left join dbo.sr_drug_forms as df on df.Id = dd.DrugFormId
left join dbo.EXP_DrugUseMethod as dum on dum.DrugDeclarationId = dd.Id
left join dbo.sr_use_methods as um on um.Id = dum.UseMethodsId
left join dbo.Employees as emp on emp.Id = dd.OwnerId
left join dbo.Units as u on u.Id = emp.OrganizationId
left join dbo.Dictionaries as d on d.Id = u.CountryId
where epfd.Id = @FinalDocumentId</SqlCommand>
