        </Parameters>
        <SqlCommand>select epfd.*
	,esd.ResultId as ResultId
	,ddos.RegNumber as RegNumber
	,ddos.ConcentrationRu as ConcentrationRu
	,ddos.Dosage as Dosage
	,dst.NameRu as SaleType
	,dd.NameRu as NameRu
	,sin.name_rus as mnnName
	,df.full_name as DrugFormRu
	,um.name as useMethod
	,u.Name as OrganizationName
	,u.LegalAddress as OrganizationLegalAddress
