        <Parameters isList="true" count="1">
          <value>_x0040_DeclarationId,DeclarationId,23,0</value>
        </Parameters>
        <SqlCommand>SELECT
	[DrugDeclarationId],
	STUFF((Select ', ' +  Tt.[Code]
From (SELECT [DrugDeclarationId]
	, Tdct.[Code]
	FROM [dbo].[EXP_DrugChangeType] AS Tct
	INNER jOIN [dbo].[EXP_DIC_ChangeType] AS Tdct ON Tdct.Id = Tct.ChangeTypeId) Tt
				Where Tt.[DrugDeclarationId] = Tdd.[DrugDeclarationId]
				ORDER BY Tt.[DrugDeclarationId]
				For XML PATH ('')
				), 1,2,'') [DosageConcentration]
FROM (
SELECT [DrugDeclarationId]
	, Tdct.[Code]
	FROM [dbo].[EXP_DrugChangeType] AS Tct
	INNER jOIN [dbo].[EXP_DIC_ChangeType] AS Tdct ON Tdct.Id = Tct.ChangeTypeId ) AS Tdd
WHERE [DrugDeclarationId] = @DeclarationId
GROUP BY [DrugDeclarationId]
</SqlCommand>
