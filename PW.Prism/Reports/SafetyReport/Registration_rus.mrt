        <NameInSource>Connection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_DeclarationId,DeclarationId,14,0</value>
        </Parameters>
        <SqlCommand>SELECT Tdo.[Id]
	,[DrugDeclarationId]
	,Tdo.[Type]
	,Tdo.[NameKz]
	,[NameRu]
	,[NameEn]      
	,[OrgManufactureTypeDicId]
	, Td.[Name] AS ManufactureTypeName
	, Td.[NameKz] AS ManufactureTypeNameKz
	,[CountryDicId]
	, Tcd.[Name] AS CountryDicName
	, Tcd.[NameKz] AS CountryDicNameKz
FROM [dbo].[EXP_DrugOrganizations] AS Tdo
INNER JOIN [dbo].[Dictionaries] AS Td ON Td.Id = Tdo.OrgManufactureTypeDicId
INNER JOIN [dbo].[Dictionaries] AS Tcd ON Tcd.Id = Tdo.CountryDicId
WHERE [DrugDeclarationId] = @DeclarationId</SqlCommand>
      </ManufactureInfo>
