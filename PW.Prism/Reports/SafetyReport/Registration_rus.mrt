      <DrugInfo Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DrugInfo</Alias>
        <Columns isList="true" count="7">
          <value>Column,System.String</value>
          <value>Number,System.String</value>
          <value>NameRu,System.String</value>
          <value>NameKz,System.String</value>
          <value>NameEn,System.String</value>
          <value>DosageConcentration,System.String</value>
          <value>DrugForm,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DrugInfo</Name>
        <NameInSource>Connection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_DeclarationId,DeclarationId,14,0</value>
        </Parameters>
        <SqlCommand>SELECT [Number]
	,[NameRu]
	,[NameKz]
	,[NameEn]
	,Tdf.[name] AS DrugForm
	,STUFF((Select '; ' + ISNULL(CONVERT(nvarchar,Tt.Dosage),'') + ' / ' + Tt.ConcentrationRu
				From [dbo].[EXP_DrugDosage] Tt
				Where Tt.DrugDeclarationId = Tdd.Id
				ORDER BY Tt.[RegNumber]
				For XML PATH ('')
				), 1,2,'') [DosageConcentration]   
  FROM [dbo].[EXP_DrugDeclaration] AS Tdd
  LEFT OUTER JOIN [dbo].[sr_dosage_forms] AS Tdf ON Tdf.id = Tdd.DrugFormId
  WHERE Tdd.Id = @DeclarationId
  GROUP BY Tdd.Id
	  ,[Number]
	  ,[NameRu]
      ,[NameKz]
      ,[NameEn]
      ,[DrugFormId]
	  ,Tdf.[name]</SqlCommand>
      </DrugInfo>
