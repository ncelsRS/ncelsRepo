		public string GetName(string s){
			string[] values = s.Split(',');
			string rezult = String.Empty;
			foreach (string d in values){
				if (d.Contains("-"))
					rezult += d.Trim().Remove((d.Trim().IndexOf('-')),d.Trim().Length - d.Trim().IndexOf('-') -1) + "\n";
