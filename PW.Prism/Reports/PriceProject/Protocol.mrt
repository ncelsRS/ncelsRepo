      <Protocol Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Protocol</Alias>
        <Columns isList="true" count="7">
          <value>OwnerName,System.String</value>
          <value>ComMembers,System.String</value>
          <value>RequesterName,System.String</value>
          <value>IsImn,System.Boolean</value>
          <value>ChiefName,System.String</value>
          <value>ProtocolDate,System.DateTime</value>
          <value>Number,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Protocol</Name>
        <NameInSource>ncels</NameInSource>
        <Parameters isList="true" count="1">
          <value>protocolId,protocolId,14,0</value>
        </Parameters>
        <SqlCommand>select
	e.FullName as OwnerName,
	p.ProtocolDate,
	p.Number,
	(select 
	(cast(e.ShortName as varchar(200))+', ') as [text()] 
		from 
			PP_ProtocolComissionMembers as cm
			inner join Employees as e on cm.EmployeeId = e.Id
		where ProtocolId = p.Id
		for XML PATH('')) as ComMembers,
	p.RequesterName,
	p.IsImn,
	ec.ShortName as ChiefName
from 
	PP_Protocols as p
	inner join Employees as e on p.OwnerId = e.Id
	left join Units as u on e.PositionId = u.Id
	left join Employees as ec on p.ChiefId = ec.Id
where p.Id = @protocolId</SqlCommand>
      </Protocol>
