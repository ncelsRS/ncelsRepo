@model PW.Ncels.Database.Models.NomenclatureModel
<dl class="pwEditRef">
    <dt>@PW.Ncels.Database.Recources.Messages.Property_Код_399__00</dt>
    <dd>
        @(Html.EditorFor(p => p.Code))
        <span data-for="Code" class="k-invalid-msg"></span>
    </dd>
    <dt>@PW.Ncels.Database.Recources.Messages.Property_Наименованиенарусском_41__00</dt>
    <dd>
        @(Html.EditorFor(p => p.Name))
        <span data-for="Name" class="k-invalid-msg"></span>
    </dd>
    <dt>@PW.Ncels.Database.Recources.Messages.Property_Наименованиенаказахском_33__00</dt>
    <dd>
        @(Html.EditorFor(p => p.NameKz))
        <span data-for="NameKz" class="k-invalid-msg"></span>
    </dd>
    <dt>@PW.Ncels.Database.Recources.Messages.Property_Департамент_259__00</dt>
    <dd>
        @(Html.Kendo().MultiSelectFor(p => p.DepartmentsId).MaxSelectedItems(1).DataValueField("Id").DataTextField("Name").DataSource(
                            source => source.Read("ListUnit", "Reference")
        ))
        <span data-for="DepartmentsId" class="k-invalid-msg"></span>
    </dd>
    <dt>@PW.Ncels.Database.Recources.Messages.Property_Примечание_264__00</dt>
    <dd>
        @(Html.EditorFor(p => p.Note))
        <span data-for="Note" class="k-invalid-msg"></span>
    </dd>
</dl>
