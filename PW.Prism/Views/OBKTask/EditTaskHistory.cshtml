@model List<PW.Ncels.Database.Models.OBK.OBKTaskHistory>

<div style="margin-left: 10px; margin-right: 10px; width: auto; height: 550px; overflow-y: scroll;">
    <div class="pwPage pwPageEx" style="overflow: hidden; width: auto;">
        <div class="panel panel-default">
            <div class="panel-heading">
                Просмотр истории
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">

                        <table id="tableExpertResearchCenter" class="table table-striped table-bordered table-hover dataTable tableResearchCenter">
                            <thead>
                                <tr>
                                    <td>Исполнитель</td>
                                    <td>Статус</td>
                                    <td>Подпись</td>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var t in Model)
                                {
                                    <tr>
                                        <td>@t.ExecutorName</td>
                                        <td>@t.StatusName</td>
                                        <td>@t.TaskSign</td>
                                    </tr>
                                }
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
