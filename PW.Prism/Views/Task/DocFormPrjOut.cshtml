@model PW.Ncels.Database.DataModel.Document

<div style="height: 256px; overflow: auto; width: 838px;">
    <table class="pwDocTable2">
        <colgroup>
            <col width="140px" />
            <col width="265px" />
            <col width="5px" />
            <col width="140px" />
            <col width="265px" />
        </colgroup>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Видисходящего_159__00</td>
            <td>
                @(Model.OutgoingType == 0 ? PW.Ncels.Database.Recources.Messages.Property_Инициативный_231__00 :
                  Model.OutgoingType == 1 ? PW.Ncels.Database.Recources.Messages.Property_Ответный_320__00 :
                  Model.OutgoingType == 2 ? PW.Ncels.Database.Recources.Messages.Property_Промежуточный_211__00 :
                  string.Empty)
            </td>
            <td></td>
            <td></td>
            <td></td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Номер_354__00</td>
            <td>
                @Model.Number
            </td>
            <td></td>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_От_393__00</td>
            <td>
                @(Model.DocumentDate.HasValue ? Model.DocumentDate.Value.ToString("dd.MM.yyyy") : string.Empty)
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Срокисполнения_115__00</td>
            <td>
                @(Model.ExecutionDate.HasValue ? Model.ExecutionDate.Value.ToString("dd.MM.yyyy") : string.Empty)
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Адресат_308__00</td>
            <td>
                @Model.CorrespondentsValue
            </td>
            <td></td>
            <td class="pwLabel">Email:</td>
            <td>
                @Model.ApplicantEmail
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Ответна_285__00</td>
            <td colspan="4">
                @Model.AnswersValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Краткоесодержание_69__00</td>
            <td colspan="4">
                @Model.Summary
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Исполнитель_222__00</td>
            <td colspan="4">
                @Model.CreatedUserValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Виддокумента_188__00</td>
            <td>
                @Model.DocumentKindDictionaryValue
            </td>
            <td></td>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Характервопроса_99__00</td>
            <td>
                @Model.QuestionDesignDictionaryValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_ЛистПрилДругое_70__00</td>
            <td>
                @string.Format("{0} / {1} / {2}", Model.PageCount, Model.CopiesCount, Model.Counters)
            </td>
            <td></td>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Языкдокумента_142__00</td>
            <td>
                @Model.LanguageDictionaryValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Видотправки_210__00</td>
            <td>
                @Model.FormSendingDictionaryValue
            </td>
            <td></td>
        </tr>
        <tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Подписано_261__00</td>
            <td colspan="4">
                @Model.SignerValue
            </td>
        </tr>

        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Дело_375__00</td>
            <td colspan="4">
                @Model.NomenclatureDictionaryValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Документ_298__00</td>
            <td colspan="4">
                <a onclick="tagAnswerClick('@Model.DestinationId','@Model.DestinationValue')">@Model.DestinationValue</a>
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Том_382__00</td>
            <td>
                @Model.Deed
            </td>
            <td></td>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Листы_360__00</td>
            <td>
                @Model.Book
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Акты_376__00</td>
            <td colspan="4">
                @Model.Akt
            </td>
        </tr>
    </table>
</div>
