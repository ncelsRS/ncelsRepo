@model PW.Ncels.Database.DataModel.Document

<div style="height: 256px; overflow: auto; width: 838px;">
    <table class="pwDocTable2">
        <colgroup>
            <col width="140px" />
            <col width="265px" />
            <col width="5px" />
            <col width="140px" />
            <col width="265px" />
        </colgroup>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Срокисполнения_115__00</td>
            <td>
                @(Model.ExecutionDate.HasValue ? Model.ExecutionDate.Value.ToString("dd.MM.yyyy") : string.Empty)
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Номер_354__00</td>
            <td>
                @Model.Number
            </td>
            <td></td>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_От_393__00</td>
            <td>
                @(Model.DocumentDate.HasValue ? Model.DocumentDate.Value.ToString("dd.MM.yyyy") : string.Empty)
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Адресат_308__00</td>
            <td colspan="4">
                @Model.ExecutorsValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Ответна_285__00</td>
            <td colspan="4">
                @Model.AnswersValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Краткоесодержание_69__00</td>
            <td colspan="4">
                @Model.Summary
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Исполнитель_222__00</td>
            <td colspan="4">
                @Model.CreatedUserValue
            </td>
        </tr>
        <tr>
            <td class="pwLabel">@PW.Ncels.Database.Recources.Messages.Property_Подписано_261__00</td>
            <td colspan="4">
                @Model.SignerValue
            </td>
        </tr>
    </table>
</div>
