@model Guid

<div id="loader@(Model)"></div>
<input type="hidden" id="signingDocId@(Model)" value="@Model" />
<input type="hidden" id="hfXmlToSign@(Model)" />
@Html.Hidden("hfStoragePath" + Model, "")
@Html.Hidden("hfKeyAlias" + Model, "")
@Html.Hidden("UserName" + Model, "")
<div class="row">
    <div class="form-group">
        <label class="control-label">Пароль</label>
        <input id="passwordCert@(Model)" type="password" class="form-control" />
    </div>
</div>
<div class=" row form-inline">
    <div class="input-group">
        <select id="keysList@(Model)" class="form-control">
        </select>
        <span class="input-group-btn">
            <button id="loadKeys@(Model)" class="btn btn-default" type="button">Загрузить ключи</button>
        </span>
    </div>
</div>
<div class="row">
    <div class="btn-toolbar" role="toolbar" style="margin-top: 20px">
        <div class="btn-group pull-right" role="group" style="margin-left: 10px">
            <button type="button" class="k-button" id="cancelSign@(Model)">Отмена</button>
        </div>
        <div class="btn-group pull-right" role="group" style="margin-left: 10px">
            <button type="button" class="k-button k-primary" id="signDoc@(Model)">Подписать</button>
        </div>
    </div>
</div>
